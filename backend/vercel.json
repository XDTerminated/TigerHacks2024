{
    "version": 2,
    "builds": [
        {
            "src": "project/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "project/wsgi.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.12"
    }
}
